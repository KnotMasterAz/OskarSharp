@COMMENT_SINGLE@ #define PLATFORM #to "Unity" #end@COMMENT_SINGLE@
@COMMENT_SINGLE@     #define LANGUAGE #to "CSHARP" #end@COMMENT_SINGLE@
@COMMENT_SINGLE@ #define VERSION #to "2.0.0" #end@COMMENT_SINGLE@
@COMMENT_SINGLE@ #define PREPROCESSOR #to "TRUE" #end@COMMENT_SINGLE@
@COMMENT_SINGLE@ // Control flow@COMMENT_SINGLE@
@COMMENT_SINGLE@ #is_equal PLATFORM #to "Unity" #begin@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace v3 #to Vector3 #end // vector3@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace v3_new #to new vector3 #end // vector3 create@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace v2 #to Vector2 #end // vector2@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace v2_new #to new vector2 #end // vector2 create@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace v4 #to Vector4 #end // vector4@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace v4_new #to new vector4 #end // vector4 create@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace trans #to Transform #end // transform@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace obj #to GameObject #end // gameobject@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace cam #to Camera #end // camera@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace show #to [SerializeField] #end // show in inspector@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace hide #to [HideInInspector] #end // hide in inspector@COMMENT_SINGLE@
@COMMENT_SINGLE@     #code@COMMENT_SINGLE@
@COMMENT_SINGLE@         using UnityEngine;@COMMENT_SINGLE@
@COMMENT_SINGLE@     #end@COMMENT_SINGLE@
@COMMENT_SINGLE@ #end@COMMENT_SINGLE@
@COMMENT_SINGLE@ #is_equal PREPROCESSOR #to "TRUE" #begin@COMMENT_SINGLE@
@COMMENT_SINGLE@     // Basic replace for code@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace pi #to 3.14 #end@COMMENT_SINGLE@
@COMMENT_SINGLE@     // Global replace@COMMENT_SINGLE@
@COMMENT_SINGLE@     #global_replace CUSTOM_NEWLINE_CHARACTER #to \n #end@COMMENT_SINGLE@
@COMMENT_SINGLE@     // Replace code example@COMMENT_SINGLE@
@COMMENT_SINGLE@     #replace@COMMENT_SINGLE@
@COMMENT_SINGLE@     halt@COMMENT_SINGLE@
@COMMENT_SINGLE@     #to@COMMENT_SINGLE@
@COMMENT_SINGLE@     while (true) {@COMMENT_SINGLE@
@COMMENT_SINGLE@         // Program halted@COMMENT_SINGLE@
@COMMENT_SINGLE@     }@COMMENT_SINGLE@
@COMMENT_SINGLE@     #end@COMMENT_SINGLE@
@COMMENT_SINGLE@ #end@COMMENT_SINGLE@
