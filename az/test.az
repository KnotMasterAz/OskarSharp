fun: float64 adder(float64 a, float64 b) {
    return a + b;
}

fun :float64 subtractor(float64 a, float64 b) {
    return a - b;
}

fun : float64 multiplier(float64 a, float64 b) {
    return a * b;
}

fun :float64 divider(float64 a, float64 b) {
    return a/b;
}
